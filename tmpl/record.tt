[% WRAPPER 'include/layout.tt' %]

<div id=databasesList>
  <table>
    <tr>
        <th>Key</th>
        <th>Value</th>
    </tr>
    [% FOR key IN record.keys() %]
        <tr>
            <td>[% key %]</td>
            <td>[% record[key] %]</td>
        </tr>
    [% END %]
  </table>
</div>

[% END %]
